@model IList<User>

@{
    ViewData["Title"] = "User manager";
}

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (User user in Model)
        {

            <tr>
                <th>@user.Id</th>
                <th>@user.Email</th>
                <th>@user.UserName</th>
                <th>@user.FirstName</th>
                <th>@user.LastName</th>
                <td><a class="btn btn-primary" role="button" asp-action="Delete" asp-route-id="@user.Id" onclick="return ConfirmDelete();">Delete</a></td>
            </tr>
        }
    </tbody>
</table>